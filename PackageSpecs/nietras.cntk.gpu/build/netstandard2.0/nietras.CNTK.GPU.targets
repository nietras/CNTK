<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <CNTKDllDir>$(MSBuildThisFileDirectory)\..\..\support\x64\</CNTKDllDir>
    <CNTKComponentVersion>2.10.0</CNTKComponentVersion>
  </PropertyGroup>
  <ItemGroup Condition="'$(Platform.ToLower())' == 'x64'">
    <None Include="$(CNTKDllDir)Release\Cntk.Core.CSBinding.dll">
      <Link>Cntk.Core.CSBinding.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Core.dll">
      <Link>Cntk.Core.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Math.dll">
      <Link>Cntk.Math.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.PerformanceProfiler.dll">
      <Link>Cntk.PerformanceProfiler.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Deserializers.Binary.dll">
      <Link>Cntk.Deserializers.Binary.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Deserializers.TextFormat.dll">
      <Link>Cntk.Deserializers.TextFormat.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Composite.dll">
      <Link>Cntk.Composite.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Deserializers.HTK.dll">
      <Link>Cntk.Deserializers.HTK.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Deserializers.Image.dll">
      <Link>Cntk.Deserializers.Image.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>